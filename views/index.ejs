<!-- main dashboard page -->
<body>
    <header>
        <div id="logo">
            <img src="./images/logo.png" alt="logo">
        </div>
        <h1>Welcome back, <%= name %>!</h1>
        <form action="/logout?_method=DELETE" method="POST">
            <button type="submit">Log Out</button>
        </form>
    </header>

<!-- Nav Bar -->
    <nav>
        <ul>
            <li>Home</li>
            <li>Video Library</li>
            <li>Blog</li>
        </ul>
    </nav>

    <!-- YouTube Search -->
<section class="youtube-search">
    <h2>Search YouTube</h2>
    <form action="/youtube-search" method="GET">
        <label for="youtubeQuery">Search:</label>
        <input type="text" id="youtubeQuery" name="q" required>
        <button type="submit">Search</button>
    </form>
</section>

<!-- Dashboard -->
    <section class="dashboard">
        <h2><%= name %>'s Dashboard</h2>
            <section class="oil">
                <!-- insert variable here -->
                <p>You need your oil changed at miles</p>
            </section>
             <section class="tires">
                <!-- insert variable here -->
                <p>You need your tires changed at miles</p>
            </section>
    </section>

<!-- Profile Overview -->
     <section class="dashboard">
        <h2><%= name %>'s Car</h2>
            <section class="car-info">
                <!-- insert car info here -->
                <p>Car Information</p>
            </section>
             <section class="car-photo">
                <!-- insert car photo here -->
                <img></img>
            </section>
    </section>

     <section class="weather">
        <!-- insert weather data here -->
         <div class="box">
            <p>Temp:  F</p>
            <p>Wind: mph</p>
            <p>Humidity: %</p>
        </div>
    </section>
    
</body>
